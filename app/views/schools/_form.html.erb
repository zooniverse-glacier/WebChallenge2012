<%= form_for @school do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :story %><br />
    <%= f.text_area :story %>
  </p>
  <p>
    <%= f.label :enabled %><br />
    <%= f.check_box :enabled %>
  </p>
  <p>
    <%= f.label :timeline_start_at %><br />
    <input type="text" data-behaviour='datepicker'>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.number_field :position %>
  </p>
  <p><%= f.submit %></p>
<% end %>
