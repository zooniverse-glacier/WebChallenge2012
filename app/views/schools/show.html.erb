<% title "School" %>

<p>
  <strong>Name:</strong>
  <%= @school.name %>
</p>
<p>
  <strong>Story:</strong>
  <%= @school.story %>
</p>
<p>
  <strong>Enabled:</strong>
  <%= @school.enabled %>
</p>
<p>
  <strong>Timeline Start At:</strong>
  <%= @school.timeline_start_at %>
</p>
<p>
  <strong>Timeline End At:</strong>
  <%= @school.timeline_end_at %>
</p>
<p>
  <strong>Coordinates:</strong>
  <%= @school.lat %>, <%= @school.lng %>
</p>

<p>
  <%= link_to "Edit", edit_school_path(@school), class: 'btn btn-mini' %>
  <%= link_to "Destroy", @school, confirm: 'Are you sure?', method: :delete, class: 'btn btn-mini' %>
  <%= link_to "View All", schools_path, class: 'btn btn-mini' %>
</p>
